<%- include layouts/admin_header.ejs %>
        <h1>Users in system</h1>
            <h4>Here you can see the results of the calls made by Users.</h4>
            <table class="table">
                <tr>
                    <th width="50px">No</th>
                    <th>Name</th>
                    <th>Last Name</th>
                    <th>Username</th>
                    <th>Action</th>
                </tr>
                               
                <% if(data.length){ 
                                
                 for(var i = 0;i < data.length;i++) { %>
                 
                <tr>
                    <td><%=(i+1)%></td>
                    <td><%=data[i].name%></td>
                    <td><%=data[i].last_name%></td>
                    <td><%=data[i].username%></td>
                    <td>
                        <a class="btn btn-warning btn-xs a-inside edit" href="../call/data/<%=data[i].iduser%>">Call Results</a>
                                             
                    </td>
                </tr>
            <% }
            
             }else{ %>
                 <tr>
                    <td colspan="3">No Users.</td>
                 </tr>
            <% } %>
                             
            </table>
        
<%- include layouts/footer.ejs %>