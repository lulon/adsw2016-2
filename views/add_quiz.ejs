<%- include layouts/admin_header.ejs %>

            <form method="post" action="/quiz/add">
                <div class="form-group">
                    <label >Name</label>
                    <input type="text" class="form-control" name="name" required>
                </div>
                <div class="form-group">
                    <label >Link</label>
                    <input type="text" class="form-control" name="link" required>
                </div>


                <button type="submit" class="btn btn-primary" name="idproject" value="<%=idproject%>">Save</button>
                <button  class="btn btn-danger" name="cancel" value="Cancel" class="cancel" onclick="window.location.href='/quiz/list/<%=idproject%>'">Cancel</button>

            </form>
            
<%- include layouts/footer.ejs %>