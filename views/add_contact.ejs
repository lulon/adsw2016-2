<%- include layouts/header.ejs %>

        <div class="page-data">
            
            <form method="post" action="/contact/add">
                <table cellpadding="11">
                <tr>
                    <td class="label">Name</td><td>: <input type="text" name="name" required></td>
                 </tr>
                 <tr>
                    <td class="label">Last Name</td><td>: <input type="text" name="last_name" required></td>
                </tr>
                <tr>
                    <td class="label">Phone</td><td>: <input type="text" name="phone" required></td>
                 </tr>
                  <tr>
                    <td class="label">To Call</td><td>: <select name="to_call"><option value="yes">Yes</option><option value="no">No</option></select> </td>
                 </tr>
                 <tr>
                    <td class="label"></td>
                    <td>
                        &nbsp; <input type="submit" name="submit" value="Save">
                       <input type="button" name="cancel" value="Cancel" class="cancel" onClick="cancelAdd()">
                    </td>
                 </tr>
                </table>
            </form>
            
        </div>
        
<%- include layouts/footer.ejs %>